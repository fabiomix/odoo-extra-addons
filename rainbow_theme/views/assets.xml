<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <template id="_assets_primary_variables" inherit_id="web._assets_primary_variables" priority="10">
        <xpath expr="//link[last()]" position="after">
            <link rel="stylesheet" type="text/scss" href="/rainbow_theme/static/src/scss/theme_colors.scss"/>
        </xpath>
    </template>
    <template id="assets_backend" inherit_id="web.assets_backend" priority="10">
        <xpath expr="//link[@rel='stylesheet'][last()]" position="after">
            <link rel="stylesheet" type="text/scss" href="/rainbow_theme/static/src/scss/overrides.scss"/>
        </xpath>
    </template>
</odoo>
